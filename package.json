{
   "name": "nest-prisma",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/index.ts",
      "start:dev": "nest start --watch",
      "start:db": "docker-compose up",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "deps": "ts-node scripts/deps.install.ts",
      "schema:load": "barrelsby --directory ./src/core/database/entities --location top --delete",
      "migrate:dev": "ts-node scripts/migrate.dev.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "migrate:generate:dev": "ts-node scripts/migrate.generate.dev.ts"
   },
   "dependencies": {
      "@aws-sdk/client-s3": "^3.733.0",
      "@aws-sdk/s3-request-presigner": "^3.733.0",
      "@nestjs/common": "^10.4.15",
      "@nestjs/config": "^3.3.0",
      "@nestjs/core": "^10.4.15",
      "@nestjs/mapped-types": "^2.1.0",
      "@nestjs/platform-express": "^10.4.15",
      "@nestjs/sequelize": "^11.0.0",
      "@nestjs/typeorm": "^11.0.0",
      "@types/multer": "^1.4.12",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.1",
      "express": "^4.21.2",
      "multer": "1.4.5-lts.1",
      "pg": "^8.13.1",
      "pg-hstore": "^2.3.4",
      "reflect-metadata": "^0.1.14",
      "rxjs": "^7.8.1",
      "sequelize": "^6.37.5",
      "sequelize-typescript": "^2.1.6",
      "serverless-http": "^3.2.0",
      "sharp": "^0.33.5",
      "typeorm": "0.3.20",
      "typeorm-model-generator": "^0.4.6",
      "uuidv4": "^6.2.13"
   },
   "devDependencies": {
      "@nestjs/cli": "^10.4.9",
      "@nestjs/schematics": "^10.2.3",
      "@nestjs/testing": "^10.4.15",
      "@types/express": "^4.17.21",
      "@types/jest": "^29.5.14",
      "@types/node": "^16.18.125",
      "@types/sequelize": "^4.28.20",
      "@types/supertest": "^6.0.2",
      "@typescript-eslint/eslint-plugin": "^8.21.0",
      "@typescript-eslint/parser": "^8.21.0",
      "barrelsby": "^2.8.1",
      "eslint": "^8.57.1",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-prettier": "^5.2.3",
      "jest": "^29.7.0",
      "prettier": "^3.4.2",
      "sequelize-cli": "^6.6.2",
      "source-map-support": "^0.5.21",
      "supertest": "^7.0.0",
      "ts-jest": "^29.2.5",
      "ts-loader": "^9.5.2",
      "ts-node": "10.9.1",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.1.3"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}